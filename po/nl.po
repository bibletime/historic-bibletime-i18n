msgstr "De grootte van de linker paginarand."

#: ../bibletime/util/cresmgr.cpp:1385
msgid "Right border"
msgstr "Rechter rand"

#: ../bibletime/util/cresmgr.cpp:1386
msgid "The size of the border on the right side of the paper."
msgstr "De grootte van de rechter paginarand."

#: ../bibletime/util/cresmgr.cpp:1397
msgid "Show daily tip"
msgstr "Toon dagelijkse tip"

#: ../bibletime/util/cresmgr.cpp:1398
