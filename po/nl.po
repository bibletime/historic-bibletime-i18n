#: ../bibletime/util/cresmgr.cpp:1366
msgid "The size of the border on the left side of the paper."
msgstr "De grootte van de linker paginarand."

#: ../bibletime/util/cresmgr.cpp:1370
msgid "Right border"
msgstr "Rechter rand"

#: ../bibletime/util/cresmgr.cpp:1371
msgid "The size of the border on the right side of the paper."
msgstr "De grootte van de rechter paginarand."

#: ../bibletime/util/cresmgr.cpp:1382
msgid "Show daily tip"
msgstr "Toon dagelijkse tip"

#: ../bibletime/util/cresmgr.cpp:1383
