msgstr "Spaties worden over de regel verdeeld doorheen de pagina."

#: ../bibletime/util/cresmgr.cpp:1283
msgid "Set the font"
msgstr "Kies lettertype"

#: ../bibletime/util/cresmgr.cpp:1284
msgid "Choose a new font for the selected text."
msgstr "Kies een nieuw lettertype voor geselecteerde tekst."

#: ../bibletime/util/cresmgr.cpp:1287
msgid "Set the font size"
msgstr "Kies grootte lettertype"

#: ../bibletime/util/cresmgr.cpp:1288
msgid "Choose a new font size for the selected text."
msgstr "Kies een grootte voor het lettertype van de gekozen tekst."

#: ../bibletime/util/cresmgr.cpp:1291
msgid "Set the color"
msgstr "Kies het kleur"

#: ../bibletime/util/cresmgr.cpp:1292
msgid "Choose a new color for the selected text."
msgstr "Kies een nieuw kleur voor de gekozen tekst."

#: ../bibletime/util/cresmgr.cpp:1304
msgid "This list contains the available print styles."
msgstr "Deze lijst bevat de beschikbare afdruk stijlen."

#: ../bibletime/util/cresmgr.cpp:1308
msgid "List of print items"
msgstr "Lijst afdruk jobs"

#: ../bibletime/util/cresmgr.cpp:1309
msgid "This is the list which contains the items to be printed."
msgstr "Deze lijst bevat de af te drukken zaken."

#: ../bibletime/util/cresmgr.cpp:1313
msgid "Move item up"
msgstr "Verplaatsen job naar boven"

#: ../bibletime/util/cresmgr.cpp:1314
msgid "Move the selected item up one line."
msgstr "Geselecteerde items een lijn stijgen"

#: ../bibletime/util/cresmgr.cpp:1318
msgid "Move item down"
msgstr "Verplaats entry naar beneden"

#: ../bibletime/util/cresmgr.cpp:1319
msgid "Move the selected item down one line."
msgstr "Verplaats de geselecteerde items een lijn naar beneden"

#: ../bibletime/util/cresmgr.cpp:1323
msgid "Delete items"
msgstr "Verwijder geselecteerde items"

#: ../bibletime/util/cresmgr.cpp:1324
msgid "Delete the selected items."
msgstr "Verwijder geselecteerde items."

#: ../bibletime/util/cresmgr.cpp:1332
msgid "New printing style"
msgstr "Nieuwe afdruk stijl"

#: ../bibletime/util/cresmgr.cpp:1333
msgid "Create a new printing style."
msgstr "Maak een nieuwe afdrukstijl."

#: ../bibletime/util/cresmgr.cpp:1338
msgid "Delete printing style"
msgstr "Printer wachtrij leegmaken"

#: ../bibletime/util/cresmgr.cpp:1339
msgid "Remove the selected printing style from the list."
msgstr "Verwijder de gekozen afdrukstijl uit de lijst."

#: ../bibletime/util/cresmgr.cpp:1344
msgid "Edit printing style"
