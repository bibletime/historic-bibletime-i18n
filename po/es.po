msgstr "Subrayado"

#: ../bibletime/util/cresmgr.cpp:1272
msgid "Toggle underlined formatting of the selected text."
msgstr "Conmutar formato subrayado del texto seleccionado."

#: ../bibletime/util/cresmgr.cpp:1277
msgid "Align left"
msgstr "Alinear a la izquierda"

#: ../bibletime/util/cresmgr.cpp:1278
